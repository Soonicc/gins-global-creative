{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/api/index.php",
      "dest": "/api/index.php",
      "continue": true
    },
    {
      "src": "/contact",
      "dest": "/public/contact.php",
      "continue": true
    },
    {
      "src": "/aboutus",
      "dest": "/public/aboutus.php",
      "continue": true
    },
    {
      "src": "/portofolio",
      "dest": "/public/portofolio.php",
      "continue": true
    },
    {
      "src": "/logo.png",
      "dest": "/public/logo.png",
      "continue": true
    },
    {
      "src": "/1.jpeg",
      "dest": "/public/1.jpeg",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}

{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "public/contact.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "public/aboutus.php": {
      "runtime": "vercel-php@0.7.1"
    },
    "public/portofolio.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/contact",
      "dest": "/public/contact.php"
    },
    {
      "src": "/aboutus",
      "dest": "/public/aboutus.php"
    },
    {
      "src": "/portofolio",
      "dest": "/public/portofolio.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}

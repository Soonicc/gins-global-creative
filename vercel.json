{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/contact.php",
            "dest": "/public/contact.php"
        },
        {
            "src": "/aboutus.php",
            "dest": "/public/aboutus.php"
        },
        {
            "src": "/portofolio.php",
            "dest": "/public/portofolio.php"
        },
          {
            "src": "/logo.png",
            "dest": "/public/logo.png"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}

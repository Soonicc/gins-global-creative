{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "api/contact",
            "dest": "public/contact.php"
        },
        {
            "src": "api/aboutus",
            "dest": "public/aboutus.php"
        },
        {
            "src": "api/portofolio",
            "dest": "public/portofolio.php"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
